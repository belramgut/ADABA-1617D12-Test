		<bean class="converters.ProductToStringConverter" />
		<bean class="converters.StringToProductConverter" />


		<bean class="converters.ShoppingGroupToStringConverter" />
		<bean class="converters.StringToShoppingGroupConverter" />


		<bean class="converters.UserAccountToStringConverter" />
		<bean class="converters.StringToUserAccountConverter" />


		<bean class="converters.UserToStringConverter" />
		<bean class="converters.StringToUserConverter" />
		
		
		<bean class="converters.WarehouseToStringConverter" />
		<bean class="converters.StringToWarehouseConverter" />
		
		<bean class="converters.PunctuationToStringConverter" />
		<bean class="converters.StringToPunctuationConverter" />

		<bean class="converters.ConfigurationToStringConverter" />
		<bean class="converters.StringToConfigurationConverter" />
		
	</util:list>

</beans>